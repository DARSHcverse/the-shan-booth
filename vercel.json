{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "https://photoboothwithshan.com.au/:path*",
      "destination": "https://www.photoboothwithshan.com.au/:path*",
      "permanent": true
    },
    {
      "source": "/blog/book-us-for-christmas-2025",
      "destination": "/blog/christmas-hire",
      "permanent": true
    },
    {
      "source": "/blog/blog-post-corporate",
      "destination": "/blog/corporate-event-trends",
      "permanent": true
    },
    {
      "source": "/blog/blog-post-photoboothtrends",
      "destination": "/blog/photo-booth-trends-melbourne-2025",
      "permanent": true
    },
    {
      "source": "/blog/blog-post-christmasprops",
      "destination": "/blog/top-10-props",
      "permanent": true
    },
    {
      "source": "/blog/blog-post-weddingphotobooth",
      "destination": "/blog/wedding-photo-booth-ideas",
      "permanent": true
    },
    {
      "source": "/blog/blog-post-photobooth-cost",
      "destination": "/blog/photobooth-cost-australia",
      "permanent": true
    },
    {
      "source": "/blog/top-5-reasons-to-hire-photo-booth-melbourne",
      "destination": "/blog/photobooth-benefits",
      "permanent": true
    },
    {
      "source": "/blog/top%2010%20props",
      "destination": "/blog/top-10-props",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|woff2|webp|png|jpg|jpeg|gif|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    }
  ]
}
